
public class CommercialFlight extends AirCraft {
	
	protected static double p;
	protected static final int minFuel = 20;
	protected static final int maxFuel = 40;
	protected static int minFuelToLand = 5;
	
	public CommercialFlight(){
		waitTime = 0;
		takeOffTime = 2;
		landingTime = 3;
		
	}

	public void incrementWaitTime() {
		waitTime++;
	}

	/* (non-Javadoc)
	 * @see AirCraft#getWaitTime()
	 */
	public int getWaitTime() {
		return waitTime;
	}

	public int getTakeOffTime() {
		
		return takeOffTime;
	}

	public int getLandingTime() {
		return landingTime;
	}
	
	public static double getP(){
		return p;
	}
	
	public static String pToString(){
		String temp = String.valueOf(getP());
		return temp;
	}
	
	public static String getName(){
		return "CommercialFlight";
	}

}
