import java.util.Random;


public class LightAircraft extends AirCraft{
	
	protected static final int minFuel = 10;
	protected static final int maxFuel = 20;
	protected static int minFuelToLand = 5;
	
	public LightAircraft(){
		waitTime = 0;
		takeOffTime = 2;
		landingTime = 3;
		
	}
	
	public static String getSpawnValue(){
		return "0.005";
	}

	public void incrementWaitTime() {
		waitTime++;
	}

	public int getWaitTime() {
		return waitTime;
	}

	public int getTakeOffTime() {
		return takeOffTime;
	}

	public int getLandingTime() {
		return landingTime;
	}

	public void fly() {
		
	}
	
	public static String getName(){
		return "LightAirCraft";
	}

}
