import java.util.PriorityQueue;


public class Runway {
	
	protected static AirCraft airCraft;
	protected static PriorityQueue<AirCraft> landingQueue;
	protected static PriorityQueue<AirCraft> takeOffQueue;
	
	public void clear(){
		landingQueue.clear();
		takeOffQueue.clear();
		System.out.print("Air Traffic has been cleared");
	}
	
	public static void addToQueue(AirCraft a){
		landingQueue.add(a);
		System.out.print("The aircraft " + getAirCraftQueue() + " has been added to the queue");
	}
	
	public static void removeFromQueue(){
		
	}
	
	public static PriorityQueue<AirCraft> getAirCraftQueue(){
		return landingQueue;
	}
	
}
