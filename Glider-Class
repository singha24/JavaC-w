
public class Glider extends AirCraft{
	
	public Glider(){
		waitTime = 0;
		takeOffTime = 3;
	}

	public void incrementWaitTime() {
		waitTime++;
	}

	public int getWaitTime() {
		return waitTime;
	}

	public int getTakeOffTime() {
		return takeOffTime;
	}

	public int getLandingTime() {
		return landingTime;
	}

	public void fly() {
		
	}
	
	public static String getSpawnValue(){
		return "0.002";
	}
	
	public static String getName(){
		return "Glider";
	}

}
